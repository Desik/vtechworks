<!-- Taken from https://wiki.duraspace.org/display/DSPACE/Release+Procedure?src=contextnavpagetreemode -->
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <!--Login info for Sonatype SnapShot repository-->
    <server>
      <id>vtechworks-snapshots</id>
      <username>${env.MAVEN_REPO_USER}</username>
      <password>${env.MAVEN_REPO_PASS}</password>
    </server>
    <!--Login info for Sonatype SnapShot repository-->
    <server>
      <id>sonatype-nexus-snapshots</id>
      <username>${env.MAVEN_REPO_USER}</username>
      <password>${env.MAVEN_REPO_PASS}</password>
    </server>
    <!--Login info for Sonatype Staging/Release repository-->
    <server>
      <id>sonatype-nexus-staging</id>
      <username>${env.MAVEN_REPO_USER}</username>
      <password>${env.MAVEN_REPO_PASS}</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>central-repo-its</id>
      <name>Local Maven Repo Proxy</name>
      <url>https://repo.lib.vt.edu/maven-central/</url>
      <mirrorOf>central</mirrorOf>
    </mirror>
    <mirror>
      <id>sonatype-repo-its</id>
      <name>Local Sonatype Release Proxy</name>
      <url>https://repo.lib.vt.edu/sonatype-releases/</url>
      <mirrorOf>sonatype-releases</mirrorOf>
    </mirror>
    <mirror>
      <id>rubygems-repo-its</id>
      <name>Local Rubygems Release Proxy</name>
      <url>https://repo.lib.vt.edu/rubygems-release/</url>
      <mirrorOf>rubygems-release</mirrorOf>
    </mirror>
    <mirror>
      <id>restlet-repo-its</id>
      <name>Local Maven Restlet Repo Proxy</name>
      <url>https://repo.lib.vt.edu/maven-restlet/</url>
      <mirrorOf>restlet</mirrorOf>
    </mirror>
  </mirrors>
</settings>
